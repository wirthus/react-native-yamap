  },
  "devDependencies": {
    "@types/react": "~18.2",
    "react": "~18.2",
    "react-native": "~0.74",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.4.0",
    "rimraf": "^5.0.7",
