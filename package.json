{
  "name": "react-native-yamap-plus",
  "version": "4.2.13",
  "description": "Yandex.MapKit and Yandex.Geocoder for react-native",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "bob build",
    "release": "release-it"
  },
  "homepage": "https://github.com/qudaeo/react-native-yamap-plus",
  "keywords": [
    "react-native",
    "mapkit",
    "yandex",
    "yamap",
    "polygon",
    "polyline",
    "marker",
    "map",
    "maps",
    "react-native-maps",
    "yandexmap",
    "yandexmaps",
    "geocoder"
  ],
  "author": "Nikita Sirotkin",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "~18.2",
    "react": "~18.2",
    "react-native": "~0.74",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.4.0",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "tsconfig.json",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!ios/RNYamap.xcodeproj/project.xcworkspace",
    "!ios/RNYamap.xcodeproj/xcuserdata",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}
