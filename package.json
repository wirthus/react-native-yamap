{
  "name": "@wirthus/react-native-yamap",
  "version": "4.3.0",
  "description": "Yandex.MapKit and Yandex.Geocoder for react-native",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index.ts",
  "react-native": "src/index.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "*.podspec",
    "tsconfig.json",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!ios/RNYamap.xcodeproj/project.xcworkspace",
    "!ios/RNYamap.xcodeproj/xcuserdata",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "homepage": "https://github.com/wirthus/react-native-yamap",
  "keywords": [
    "react-native",
    "mapkit",
    "yandex",
    "yamap",
    "polygon",
    "polyline",
    "marker",
    "map",
    "maps",
    "react-native-maps",
    "yandexmap",
    "yandexmaps",
    "geocoder"
  ],
  "author": "Nikita Sirotkin",
  "license": "MIT",
  "scripts": {
    "build": "bob build",
    "check": "tsc --noEmit",
    "clean": "rimraf lib",
    "lint:fix": "eslint --fix .",
    "lint": "eslint .",
    "release:dryRun": "release-it --dry-run",
    "release": "release-it"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.74.84",
    "@react-native/eslint-plugin": "^0.74.84",
    "@react-native/typescript-config": "^0.74.84",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsdoc": "^48.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-wirthus": "^1.1.0",
    "react": "^18.3.1",
    "react-native": "~0.74.2",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^17.4.0",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
